.header {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.3);
  height: 65px;
  background-color: #fff;
  z-index: 999;
}

.headerInner {
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-flow: nowrap row;

  padding: 0 20px;

  @media (max-width: 992px) {
    padding: 0 10px;
  }
}

.logo {
  height: 65px;
  padding: 10px;
}

.img {
  width: 116px;
  height: 40px;
}
